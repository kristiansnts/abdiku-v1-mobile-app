{
  "expo": {
    "name": "Abdiku by Payrollkami",
    "slug": "abdiku-v1-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/abdiku-thumbnail.png",
    "scheme": "abdikuv1mobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to verify your attendance at work locations.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to verify your attendance at work locations."
      },
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY"
      },
      "bundleIdentifier": "com.kristiansnts.abdikuv1mobileapp",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "notification": {
      "icon": "./assets/images/abdiku-thumbnail.png",
      "color": "#0EA5E9",
      "androidMode": "default",
      "androidCollapsedTitle": "Abdiku Notifications"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "usesCleartextTraffic": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },
      "package": "com.kristiansnts.abdikuv1mobileapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to your location to verify your attendance at work locations.",
          "locationWhenInUsePermission": "This app needs access to your location to verify your attendance at work locations."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/abdiku-thumbnail.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "@react-native-community/datetimepicker",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/abdiku-logo.png",
          "color": "#0EA5E9",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4b88983b-c04a-4c67-a314-c5c96444ecc8"
      }
    },
    "owner": "kristiansnts"
  }
}